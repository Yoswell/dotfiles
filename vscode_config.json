/*/ Code custom vscode_config.json configuration /*/

{
    "editor.minimap.enabled": false,
    "editor.fontFamily": "JetBrains Mono SemiBold",
    "editor.overviewRulerBorder": false,
    "workbench.settings.applyToAllProfiles": [],
    "workbench.colorCustomizations": {},
    "editor.hover.enabled": false,
    "workbench.tree.indent": 10,
    "editor.cursorBlinking": "expand",
    "editor.guides.indentation": false,
    "editor.scrollbar.horizontalScrollbarSize": 8,
    "editor.scrollbar.verticalScrollbarSize": 8,
    "window.density.editorTabHeight": "compact",
    "editor.fastScrollSensitivity": 15,
    "editor.mouseWheelScrollSensitivity": 3,
    "editor.scrollBeyondLastLine": false,
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": false,
    "editor.stickyScroll.scrollWithEditor": false,
    "editor.screenReaderAnnounceInlineSuggestion": false,
    "workbench.tree.enableStickyScroll": false,
    "editor.minimap.showMarkSectionHeaders": false,
    "editor.minimap.showRegionSectionHeaders": false,
    "editor.minimap.renderCharacters": false,
    "window.menuBarVisibility": "compact",
    "extensions.ignoreRecommendations": true,
    "workbench.editor.enablePreview": false,
    "debug.console.fontSize": 12,
    "scm.inputFontSize": 12,
    "terminal.integrated.fontSize": 12,
    "chat.editor.fontSize": 11,
    "markdown.preview.fontSize": 12,
    "editor.renderLineHighlight": "none",
    "symbols.hidesExplorerArrows": false,
    "terminal.integrated.tabs.location": "left",
    "workbench.editor.scrollToSwitchTabs": true,
    "java.completion.favoriteStaticMembers": [
        "org.junit.Assert.*",
        "org.junit.Assume.*",
        "org.junit.jupiter.api.Assertions.*",
        "org.junit.jupiter.api.Assumptions.*",
        "org.junit.jupiter.api.DynamicContainer.*",
        "org.junit.jupiter.api.DynamicTest.*",
        "org.mockito.Mockito.*",
        "org.mockito.ArgumentMatchers.*",
        "org.mockito.Answers.*"
    ],
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorStyleInactive": "line",
    "terminal.integrated.cursorWidth": 2,
    "explorer.confirmDragAndDrop": false,
    "workbench.iconTheme": "let-icons",
    "explorer.confirmDelete": false,
    "workbench.colorTheme": "Bearded Theme Monokai Black",
    "window.titleBarStyle": "native",
    "workbench.activityBar.location": "bottom",
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
        "*.cs": "${capture}.designer.cs,${capture}.g.cs,${capture}.generated.cs,${capture}.run.json,${capture}.settings.json,${capture}.settings.*.json",
        "*.csproj": "${capture}.csproj.user",
        "*.json": "${capture}.Development.json",
        "*.cshtml": "${capture}.cshtml.cs,${capture}.cshtml.css",
        "*.razor": "${capture}.razor.cs,${capture}.razor.css",
        "*.resx": "${capture}.Designer.cs",
        "*.xaml": "${capture}.xaml.cs",
        "*.sqlite": "${capture}.${extname}-*",
        "*.db": "${capture}.${extname}-*",
        "*.sqlite3": "${capture}.${extname}-*",
        "*.db3": "${capture}.${extname}-*",
        "*.sdb": "${capture}.${extname}-*",
        "*.s3db": "${capture}.${extname}-*"
    }
}
